# This file implements regression tests for SQLite library.
#

set testdir [file dirname $argv0]
source $testdir/tester.tcl
set testprefix hct_recover3

hct_reset_db

do_execsql_test 1.0 {
  CREATE TABLE t1(a, b);
  INSERT INTO t1 VALUES('one', 'two');
  INSERT INTO t1 VALUES('three', 'four');
}

db close
sqlite3 db test.db
breakpoint
do_execsql_test 1.1 {
  BEGIN;
    SELECT * FROM t1;
  COMMIT;
} {one two three four}


finish_test


